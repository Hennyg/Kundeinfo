{
  "routes": [
    { "route": "/admin.html", "allowedRoles": ["authenticated"] },
    { "route": "/adminedit.html", "allowedRoles": ["authenticated"] },
    { "route": "/adminsurvey.html", "allowedRoles": ["authenticated"] },

    { "route": "/api/*", "allowedRoles": ["authenticated"] },
    { "route": "/survey/*", "allowedRoles": ["anonymous", "authenticated"] }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/assets/*",
      "/api/*",
      "/admin.html",
      "/adminedit.html",
      "/adminsurvey.html",
      "/survey/*"
    ]
  },
  "globalHeaders": {
    "Cache-Control": "no-store"
  }
}
