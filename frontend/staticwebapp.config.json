{
  "routes": [
    { "route": "/adminedit.html", "allowedRoles": ["authenticated"] },
    { "route": "/api/*", "allowedRoles": ["authenticated"] },
    { "route": "/survey/*", "allowedRoles": ["anonymous", "authenticated"] }
  ],
  "responseOverrides": {
    "401": { "rewrite": "/.auth/login/aad?post_login_redirect_uri=/adminedit.html" }
  },
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/assets/*", "/api/*", "/adminedit.html", "/survey/*"]
  }
}
